{
	"name": "@repo/shared",
	"version": "0.0.0",
	"private": true,
	"main": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./enums": {
			"types": "./src/enums/index.ts",
			"default": "./dist/enums/index.js"
		},
		"./constants": {
			"types": "./src/constants/index.ts",
			"default": "./dist/constants/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "biome check",
		"format": "biome format --write",
		"check-types": "tsc --noEmit"
	},
	"devDependencies": {
		"typescript": "5.9.3",
		"@repo/typescript-config": "workspace:*"
	}
}
